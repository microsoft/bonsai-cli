# from http://coverage.readthedocs.io/en/latest/config.html
[run]
branch = True
source = bonsai_cli
parallel = True

[report]
# Regexes for lines to exclude from consideration
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain about no-ops
    pass

[html]
directory = test-results/htmlcov
